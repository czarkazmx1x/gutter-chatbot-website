# Environment Configuration
# Copy this file to .env and update with your actual values

# N8N Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.app.n8n.cloud/webhook/gutter-chatbot
N8N_API_KEY=your-n8n-api-key-here

# Cloudflare Configuration
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
CLOUDFLARE_ZONE_ID=your-zone-id
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_PROJECT_NAME=gutter-website

# Website Configuration
SITE_DOMAIN=yourguttercompany.com
BUSINESS_NAME=Your Gutter Company
BUSINESS_PHONE=+1-555-123-4567
BUSINESS_EMAIL=info@yourguttercompany.com

# Optional: Analytics & Tracking
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
FACEBOOK_PIXEL_ID=123456789

# Optional: Integration APIs
OPENAI_API_KEY=your-openai-key-for-advanced-ai
GOOGLE_MAPS_API_KEY=your-google-maps-key

# Development Settings
NODE_ENV=production
DEBUG=false
